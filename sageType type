[32m+[m[32mvar connectingElement = document.querySelector('.connecting');[m
[32m+[m
[32m+[m[32mvar stompClient = null;[m
[32m+[m[32mvar username = null;[m
[32m+[m
[32m+[m[32mvar colors = [[m
[32m+[m[32m    '#2196F3', '#32c787', '#00BCD4', '#ff5652',[m
[32m+[m[32m    '#ffc107', '#ff85af', '#FF9800', '#39bbb0'[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32mfunction connect(event) {[m
[32m+[m[32m    username = document.querySelector('#name').value.trim();[m
[32m+[m
[32m+[m[32m    if(username) {[m
[32m+[m[32m        usernamePage.classList.add('hidden');[m
[32m+[m[32m        chatPage.classList.remove('hidden');[m
[32m+[m
[32m+[m[32m        var socket = new SockJS('/ws');[m
[32m+[m[32m        stompClient = Stomp.over(socket);[m
[32m+[m
[32m+[m[32m        stompClient.connect({}, onConnected, onError);[m
[32m+[m[32m    }[m
[32m+[m[32m    event.preventDefault();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction onConnected() {[m
[32m+[m[32m    // Subscribe to the Public Topic[m
[32m+[m[32m    stompClient.subscribe('/topic/sendMsg', onMessageReceived);[m
[32m+[m
[32m+[m[32m    // Tell your username to the server[m
[32m+[m[32m    stompClient.send('/app/addUser',[m
[32m+[m[32m        {},[m
[32m+[m[32m        JSON.stringify({sender: username, type: 'JOIN'})[m
[32m+[m[32m    )[m
[32m+[m
[32m+[m[32m    connectingElement.classList.add('hidden');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction onError(error) {[m
[32m+[m[32m    connectingElement.textContent = 'Could not connect to WebSocket server. Please refresh this page to try again!';[m
[32m+[m[32m    connectingElement.style.color = 'red';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction sendMessage(event) {[m
[32m+[m[32m    var messageContent = messageInput.value.trim();[m
[32m+[m[32m    if(messageContent && stompClient) {[m
[32m+[m[32m        var chatMessage = {[m
[32m+[m[32m            sender: username,[m
[32m+[m[32m            content: messageInput.value,[m
[32m+[m[32m            type: 'CHAT'[m
[32m+[m[32m        };[m
[32m+[m[32m        stompClient.send('/app/sendMsg', {}, JSON.stringify(chatMessage));[m
[32m+[m[32m        messageInput.value = '';[m
[32m+[m[32m    }[m
[32m+[m[32m    event.preventDefault();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction onMessageReceived(payload) {[m
[32m+[m[32m    var message = JSON.parse(payload.body);[m
[32m+[m
[32m+[m[32m    var messageElement = document.createElement('li');[m
[32m+[m
[32m+[m[32m    if(message.type === 'JOIN') {[m
[32m+[m[32m        messageElement.classList.add('event-message');[m
[32m+[m[32m        message.content = message.sender + ' joined!';[m
[32m+[m[32m    } else if (message.type === 'LEAVE') {[m
[32m+[m[32m        messageElement.classList.add('event-message');[m
[32m+[m[32m        message.content = message.sender + ' left!';[m
[32m+[m[32m    } else {[m
[32m+[m[32m        messageElement.classList.add('chat-message');[m
[32m+[m
[32m+[m[32m        var avatarElement = document.createElement('i');[m
[32m+[m[32m        var avatarText = document.createTextNode(message.sender[0]);[m
[32m+[m[32m        avatarElement.appendChild(avatarText);[m
[32m+[m[32m        avatarElement.style['background-color'] = getAvatarColor(message.sender);[m
[32m+[m
[32m+[m[32m        messageElement.appendChild(avatarElement);[m
[32m+[m
[32m+[m[32m        var usernameElement = document.createElement('span');[m
[32m+[m[32m        var usernameText = document.createTextNode(message.sender);[m
[32m+[m[32m        usernameElement.appendChild(usernameText);[m
[32m+[m[32m        messageElement.appendChild(usernameElement);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var textElement = document.createElement('p');[m
[32m+[m[32m    var messageText = document.createTextNode(message.content);[m
[32m+[m[32m    textElement.appendChild(messageText);[m
[32m+[m
[32m+[m[32m    messageElement.appendChild(textElement);[m
[32m+[m
[32m+[m[32m    messageArea.appendChild(messageElement);[m
[32m+[m[32m    messageArea.scrollTop = messageArea.scrollHeight;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction getAvatarColor(messageSender) {[m
[32m+[m[32m    var hash = 0;[m
[32m+[m[32m    for (var i = 0; i < messageSender.length; i++) {[m
[32m+[m[32m        hash = 31 * hash + messageSender.charCodeAt(i);[m
[32m+[m[32m    }[m
[32m+[m[32m    var index = Math.abs(hash % colors.length);[m
[32m+[m[32m    return colors[index];[m
 }[m
