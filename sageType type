[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar usernamePage = document.querySelector('#username-page');[m
[32m+[m[32mvar chatPage = document.querySelector('#chat-page');[m
[32m+[m[32mvar usernameForm = document.querySelector('#usernameForm');[m
[32m+[m[32mvar messageForm = document.querySelector('#messageForm');[m
[32m+[m[32mvar messageInput = document.querySelector('#message');[m
